{
  "rewrites": [
    {
      "source": "!/static/*",
      "destination": "/index.html"
    },
    {
      "source": "!/manifest.json",
      "destination": "/index.html"
    },
    {
      "source": "!/**/*.{js,css,png,jpg,jpeg,gif,ico,svg,woff,woff2,ttf,eot}",
      "destination": "/index.html"
    }
  ]
}